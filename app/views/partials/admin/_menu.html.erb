<div class="row">
  <div class="col s12">
    <ul class="admin_menu" >
      <li class="tab col s2 menu_admin">
        <%= link_to t("page.admin.menu.users"), admin_users_path %>
      </li>
      <li class="tab col s2 menu_admin">
        <%= link_to t("page.admin.menu.subjects"), admin_subjects_path %>
      </li>
      <li class="tab col s2 menu_admin">
        <%= link_to t("page.admin.menu.questions"), admin_questions_path %>
      </li>
      <li class="tab col s2 menu_admin">
        <%= link_to t("page.admin.menu.suggestions"), controller: "admin/questions",
          action: "index", suggest_question: 1 %>
      </li>
      <li class="tab col s2 menu_admin">
        <%= link_to t("page.admin.menu.exams"), admin_examinations_path %>
      </li>
    </ul>
  </div>
  <hr />
  <div class="row">
    <div class="col m10 offset-m1">
      <h4 class="center-align"><%= yield :page_title %></h4>
      <% flash.each do |message_type, message| %>
        <div class="alert alert-<%= message_type %>"><%= message %></div>
      <% end %>
    </div>
  </div>
  <hr />
  <div class="content-page">
    <%= yield %>
  </div>
</div>


